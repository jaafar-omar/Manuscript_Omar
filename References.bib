@article{chee2015,
  title    = {Real-time approaches for characterization of fully and partially scanned canopies in groves},
  journal  = {Computers and Electronics in Agriculture},
  volume   = {118},
  pages    = {361-371},
  year     = {2015},
  issn     = {0168-1699},
  doi      = {https://doi.org/10.1016/j.compag.2015.09.017},
  url      = {https://www.sciencedirect.com/science/article/pii/S0168169915002926},
  author   = {Fernando A. {Auat Cheein} and José Guivant and Ricardo Sanz and Alexandre Escolà and Francisco Yandún and Miguel Torres-Torriti and Joan R. Rosell-Polo},
  keywords = {Crown volume, LiDAR sensor, Mobile terrestrial laser scanner, Agricultural robotics},
  abstract = {Efficient information management in orchard characterization leads to more efficient agricultural processes. In this brief, a set of computational geometry methods are presented and evaluated for orchard characterization; in particular, for the estimation of canopy volume and shape in groves and orchards using a LiDAR (Light Detection And Ranging) sensor mounted on an agricultural service unit. The proposed approaches were evaluated and validated in the field, showing they are convergent in the estimation process and that they are able to estimate the crown volume for fully scanned canopies in real time; for partially observed tree crowns, accuracy decreases up to 30% (the worst case). The latter is the major contribution of this brief since it implies that the automated service unit does not need to cover all alley-ways for an accurate modeling of the orchard, thus saving valuable resources.}
}

@incollection{chua2017,
  title     = {Chapter Four - Software and Data Format},
  editor    = {Chee Kai Chua and Chee How Wong and Wai Yee Yeong},
  booktitle = {Standards, Quality Control, and Measurement Sciences in 3D Printing and Additive Manufacturing},
  publisher = {Academic Press},
  pages     = {75-94},
  year      = {2017},
  isbn      = {978-0-12-813489-4},
  doi       = {https://doi.org/10.1016/B978-0-12-813489-4.00004-0},
  url       = {https://www.sciencedirect.com/science/article/pii/B9780128134894000040},
  author    = {Chee Kai Chua and Chee How Wong and Wai Yee Yeong},
  keywords  = {additive manufacturing, AMF, 3MF, computer aided design, slicing, medical devices, DICOM},
  abstract  = {To produce a part through additive manufacturing (AM), an AM system must be able to receive digital model data and slice the model into the required layers. However, there are many different data formats in the market, each with its own advantages and disadvantages, with STereoLithography (STL) being the de facto file format standard. These three-dimensional (3D) model data are obtained either through computer-aided design (CAD) or 3D scanning. Depending on the scanning resolution, parts that are scanned may not be good for direct AM fabrication, requiring the user to repair or patch the model to a satisfactory standard before printing. The user has to understand the importance of ensuring that the parts that are scanned and printed consist of the features that are of crucial importance. A poor selection of slicing parameters may result in loss of features while over conservative slicing parameters will result in a longer printing time. In the medical industry, scanning involves using radiation to study the anatomy of the human body. These images are two-dimensional (2D) in nature, which may not carry enough information. Multiple images are taken and stacked together to form 3D models of the human anatomy.}
}

